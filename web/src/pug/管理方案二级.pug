include common/common.pug
+head({title: '管理方案二级', bgClass: 'bgF7', links: ['viewer.min.css'], scripts: ['viewer-jquery.min.js', 'mangerTwoStep.js']})
    .wrap.wrap--bg
        .top 欢迎，设计师
            em.top__user-name 轩亦辰
            | |
            a(href="javascript:;").top__btn.top__btn--blue 退出
        +headBar
        .content.scheme
            .screening
                .screening__item
                    label.screening__label 时间：
                    select.screening__select
                        option 全部
                        option 2017
                        option 2016
                .screening__item
                    label.screening__label 城市：
                    select.screening__select
                        option 全部
                        option 北京-朝阳
                        option 河南-郑州-新乡
                .screening__item
                    label.screening__label 小区：
                    select.screening__select
                        option 全部
                        option 北京-朝阳
                        option 河南-郑州-新乡
            ul.schemeList#schemeList
                - for (var x = 0; x < 6; x++)
                    li.schemeList__item()
                        .infoSimple
                            img(src="../images/temp/glfa.png", alt="").infoSimple__img
                            .infoSimple__txtBox
                                .pbox
                                    p.infoSimple__txt.infoSimple__txt--tit 
                                        span 紫金新干线10号楼3单元20#{x}
                                        a(href="javascript:;").editDoorModel 编辑户型信息
                                .pbox
                                    p.infoSimple__txt 所属城市：北京-朝阳
                                    p.infoSimple__txt 所属小区：紫金新干线
                                    p.infoSimple__txt 3室2厅2卫1厨
                                        span.m--l30 130平
                                .pbox
                                    p.infoSimple__txt 方案数：
                                        em.txtColor--blue 12
                            .infoSimple__txtBox--poa
                                a(href="javascript:;").infoSimple__open
                                a(href="javascript:;").addPlan  添加方案
                        .infoDetail
                            - for (var a = 0; a < 3; a++)
                                .infoDetail__item
                                    .infoDetail__head
                                        .infoDetail__left
                                            h4.infoDetail__tit 方案#{a + 1}：简约第一版 (简约)
                                                a(href="管理方案.html").infoDetail__style 编辑
                                            .userInof
                                                span.userInof__item 客户：轩亦辰
                                                span.userInof__item 电话：18610996127
                                                span.userInof__item 最后修改:2017-06-22

                                        a(href="管理方案.html").infoDetail__btn 编辑方案
                                    .infoDetail__main
                                        .detailBlock.detailBlock--photo
                                            .detailBlock__head
                                                h5.detailBlock__tit 效果图
                                                a(href="管理方案.html").detailBlock__all.detailBlock__all--unread 查看全部
                                            ul.detailList
                                                - for (var y = 0; y < 3; y++)
                                                    li.detailList__item
                                                        a(href="2主页面-方案管理-二级-查看.html", class= y ==2 ? 'img_default': "").detailList__link
                                                            -if(y == 2) {
                                                                img(src="../images/public/img_default.png", alt="").detailList__img
                                                            -} else {
                                                                img(src="../images/temp/glfa.png", alt="", data-original="../images/public/img_default.png").detailList__img
                                                            -}
                                                        p.detailList__txt 暂无图片
                                        .detailBlock.detailBlock--vedio
                                            .detailBlock__head
                                                h5.detailBlock__tit 视频
                                                a(href="管理方案.html").detailBlock__all 查看全部
                                            ul.detailList
                                                - for (var y = 0; y < 3; y++)
                                                    li.detailList__item
                                                        -if(y == 2) {
                                                            a(href="javascript:;", class='detailList__link--default vedio_default').detailList__link.vedioIcon
                                                            img(src="../images/public/vedio_default.png", alt="").detailList__img
                                                            p.detailList__txt 暂无视频
                                                        -} else {
                                                            a(href="javascript:;", src="/images/login/bg.mp4").detailList__link.vedioIcon
                                                                img(src= "../images/temp/glfa.png", alt="").detailList__img
                                                            p.detailList__txt 暂无视频
                                                        -}
            a(href="javascript:;").loadMore#loadMore  点击加载更多
            a(href="#").gotoTop 返回顶部
    input(type="hidden", id="loadMoreURI", value="/")
    +pubPop
    +pubPopLayout({isNone: true, ov: true, id:"editPlan"})
        .pubPopHead
            h3.pubPopHead__tit 添加方案
        ul.eidtFamilyInfo(style="width: 458px")
            li.eidtFamilyInfo__item
                label.eidtFamilyInfo__label 户型小区：
                .eidtBox
                    input.eidtInput(value="紫金新干线10号楼201", disabled).eidtInput--noborder
            li.eidtFamilyInfo__item
                label.eidtFamilyInfo__label 方案名称：
                .eidtBox
                    input.eidtInput(value="紫金新干线10号楼201")
            li.eidtFamilyInfo__item
                label.eidtFamilyInfo__label 方案风格：
                .eidtBox
                    .stylesTags
                        a(href="javascript:;").stylesTag 欧美风
                        a(href="javascript:;").stylesTag 北欧风
                        a(href="javascript:;").stylesTag.stylesTag--active 中国风
                        a(href="javascript:;").stylesTag 地中海风
                        a(href="javascript:;").stylesTag 复古风
                        a(href="javascript:;").stylesTag 现代风
                        a(href="javascript:;").stylesTag 简约风
            li.eidtFamilyInfo__item
                label.eidtFamilyInfo__label 客户姓名：
                .eidtBox
                    input.eidtInput(value="")
                    p.error 客户姓名填写后即不可更改请慎重填写
            li.eidtFamilyInfo__item
                label.eidtFamilyInfo__label 客户电话：
                .eidtBox
                    input.eidtInput(value="")
                    p.error 客户电话填写后即不可更改，且该电话为客户的手机端账号，请重填写。

        .price
            p.price__tips 当前添加价格(钢镚)
            h3.price__num 1600
        p.curprice 当前余额：2457钢镚
        a(href="javascript:;", confirm="true").btn.determineBtn 确  定
        //-a(href="javascript:;").btn.btn--gray.determineBtn 确  定
        //-a(href="javascript:;").btn.determineBtn 余额不足，去充值
        p.error 电话格式错误

    +pubPopLayout({isNone: true, id: "editHouseType"})
        .pubPopHead
            h3.pubPopHead__tit 编辑户型信息
        ul.eidtFamilyInfo
            li.eidtFamilyInfo__item
                label.eidtFamilyInfo__label 户型名称：
                .eidtBox
                    input.eidtInput(value="新湖家园3号楼201")
            li.eidtFamilyInfo__item
                label.eidtFamilyInfo__label 所在城市：
                .eidtBox
                    .lenovo.lenovo--active1
                        input(type="text", disabled, value="河南省").lenovo__inp
                        ul.lenovoList
                            li.lenovoList__item 河南省
                            li.lenovoList__item 河南省
                            li.lenovoList__item 河南省
                            li.lenovoList__item 河南省
                    .lenovo
                        input(type="text", disabled, value="郑州市").lenovo__inp
                        ul.lenovoList
                            li.lenovoList__item 郑州市
                            li.lenovoList__item 郑州市
                            li.lenovoList__item 郑州市
                    .lenovo
                        input(type="text", disabled, value="新郑市").lenovo__inp
                        ul.lenovoList
                            li.lenovoList__item 新郑市
                            li.lenovoList__item 新郑市
                            li.lenovoList__item 新郑市
                            li.lenovoList__item 新郑市
            li.eidtFamilyInfo__item
                label.eidtFamilyInfo__label 选择小区：
                .eidtBox
                    .lenovo.W300.lenovo--active1
                        input(type="text").lenovo__inp
                        ul.lenovoList
                            li.lenovoList__item 和平小区
                            li.lenovoList__item 和平小区
                            li.lenovoList__item 和平小区
                    span.eidt__tips （选择或输入小区）
            li.eidtFamilyInfo__item
                label.eidtFamilyInfo__label 选择格局：
                .eidtBox
                    input.eidtInput.eidtInput--xs(type="tel")
                    span.eidt__tips 室
                    input.eidtInput.eidtInput--xs(type="tel")
                    span.eidt__tips 厅
                    input.eidtInput.eidtInput--xs(type="tel")
                    span.eidt__tips 卫
                    input.eidtInput.eidtInput--xs(type="tel")
                    span.eidt__tips 厨
            li.eidtFamilyInfo__item
                label.eidtFamilyInfo__label 平方数：
                .eidtBox
                    input.eidtInput(value="150",type="number")
                    span.eidt__tips ㎡
        a(href="javascript:;", confirm="true").btn.btn-blue.eidtBtn 确定
